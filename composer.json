{
    "name": "stevebauman/unfinalize",
    "description": "Permanently remove final keywords from vendor packages.",
    "keywords": ["final", "unfinalize", "php-cs-fixer", "fixer"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/stevebauman/unfinalize/issues",
        "source": "https://github.com/stevebauman/unfinalize"
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nikic/php-parser": "^4.17"
    },
    "require-dev": {
        "laravel-zero/framework": "^10.0.2",
        "nunomaduro/termwind": "^1.15.1",
        "friendsofphp/php-cs-fixer": "^3.16",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5",
        "spatie/ray": "^1.39"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/unfinalize"]
}
